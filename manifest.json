{
	"manifest_version": 3,
	"name": "Dynamic Content Tracker",
	"version": "1.0",
	"permissions": ["tabs", "notifications", "storage", "scripting", "webNavigation"],
	"background": {
		"service_worker": "background.js"
	},
	"action": {
		"default_popup": "popup.html"
	},
	"host_permissions": ["http://*/*", "https://*/*"]
}
